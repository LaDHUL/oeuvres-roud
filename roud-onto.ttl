                      rdf:type owl:Restriction ;
                      owl:onProperty roud-oeuvres:hasPageImageValue ;  
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                    ],
                    [
                      rdf:type owl:Restriction ;
                      owl:onProperty roud-oeuvres:pageIsPartOfWrittenSource ;
                      owl:cardinality "1"^^xsd:nonNegativeInteger                    
                    ],
                    [
                      rdf:type owl:Restriction ;
                      owl:onProperty roud-oeuvres:pageIsPartOfWrittenSourceValue ;
                      owl:cardinality "1"^^xsd:nonNegativeInteger                    
