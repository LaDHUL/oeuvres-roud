#### DOCUMENT DU FONDS D'ARCHIVE - abstract class ##############
roud-oeuvres:WrittenSource    rdf:type owl:Class ;
                              rdfs:subClassOf knora-base:Resource ,
                              
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceIsInArchive ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceHasTitle ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceHasShelfmark ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceHasOldShelfmark ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                   
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:hasEditorialSet ;
                                owl:minCardinality "0"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;  
                                owl:onProperty roud-oeuvres:hasAnnotation ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                 
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:hasPublishedReference ;
                                owl:minCardinality "0"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:hasPublishedReferenceValue ;
                                owl:minCardinality "0"^^xsd:nonNegativeInteger
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:hasGeneticStage ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                              ],
                              [
                                rdf:type owl:Restriction ;  
                                owl:onProperty roud-oeuvres:isPhotocopy ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger               
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceIsDigitized ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                    
                              ],
                              [
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:hasPublicComment ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                              ],
                              [ 
                                rdf:type owl:Restriction ;
                                owl:onProperty roud-oeuvres:writtenSourceHasInternalComment ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                              ];  ## isPreviousStageOf - minCardinality 0 - eliminato, che era ??

                              rdfs:label  "(Classe abstraite, ne pas utiliser. Document)"@fr ,
                                          "(Abstract class, do not use. Document)"@en ,
                                          "(Classe astratta, non usare. Documento)"@it ;

                              rdfs:comment  """NE PAS UTILISER. Tout document écrit conservé dans un archive (CRLR ou autre). Il s'agit d'une classe abstraite, voir plutôt ses sub-classes."""@fr ,
                                            """DO NOT USE. Any written document contained in an archive (CRLR or other). This is an abstract class, see its sub-classes."""@en .



#### MANUSCRIPT
roud-oeuvres:Manuscript   rdf:type owl:Class ;

                          rdfs:subClassOf roud-oeuvres:WrittenSource ;

                          rdfs:subClassOf knora-base:Resource ,
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasDocumentType ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:manuscriptHasDate ;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger       
                          ],                         
                          [
                            rdf:type owl:Restriction ;   
                            owl:onProperty roud-oeuvres:manuscriptHasDateEstablished ;   ## add class DateEstablished, with Date and Comment ??
                            owl:minCardinality "0"
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasSupportType ;    ## cardinality of SupportType and SupportInfo should be 1, when all fiches will be done.
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasSupportInfo ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasWritingTool ;   ## cardinality of WritingTool and WritingColor should be 1, when all fiches will be done.
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasWritingColor ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasOtherWritingTool ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasDatationComment ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger                      
                          ];
                          rdfs:label  "Document d'archive"@fr ,
                                      "Archival document"@en ,
                                      "Documento d'archivio"@it ;
                                      
                          rdfs:comment  """Un manuscrit ou un dactylogramme qui se trouve dans un fonds d’archive."""@fr ,
                                        """A manuscript or a typescript in an archive."""@en .

                          
                    

#### PAGE ################
