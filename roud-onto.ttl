roud-oeuvres:isInArchive    rdf:type  owl:ObjectProperty ;
                        
                            rdfs:subPropertyOf knora-base:hasValue ;

                            knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                            knora-base:objectClassConstraint knora-base:ListValue ;
                            
                            rdfs:label  "Position dans les archives"@fr ,
                                        "Location in the archives"@en ,
                                        "Posizione nei fondi d'archivio"@it ;

                            rdfs:comment  "Archive où le document est conservé et position du document dans l'archive"@fr .



roud-oeuvres:hasTitle   rdf:type owl:ObjectProperty ;

                        rdfs:subPropertyOf  knora-base:hasValue ;

                        knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                        knora-base:objectClassConstraint knora-base:TextValue ;

                        rdfs:label  "Titre"@fr ,
                                    "Title"@en ,
                                    "Titolo"@it ;

                        rdfs:comment  "Le titre du document"@fr .




roud-oeuvres:hasShelfNumber   rdf:type owl:ObjectProperty ;

                              rdfs:subPropertyOf  knora-base:hasValue ;

                              knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                              knora-base:objectClassConstraint knora-base:TextValue ;

                              rdfs:label  "Cote"@fr ,
                                          "Shelf number"@en ,
                                          "Segnatura"@it ;

                              rdfs:comment  "La cote du document"@fr .

                  


roud-oeuvres:hasOldShelfNumber    rdf:type owl:ObjectProperty ;
                          
                                  rdfs:subPropertyOf  knora-base:hasValue ;

                                  knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                                  knora-base:objectClassConstraint knora-base:TextValue ;

                                  rdfs:label  "Ancienne cote"@fr ,
                                              "Previous shelf number"@en ,
                                              "Segnatura precedente"@it ;

                                  rdfs:comment  "Ancienne cote du document"@fr .


roud-oeuvres:hasEditorialSet  rdf:type  owl:ObjectProperty ;
                      
                              rdfs:subPropertyOf  knora-base:hasValue ;

                              knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                              knora-base:objectClassConstraint knora-base:ListValue ;
                              ## See  roud-data-lists.ttl

                              rdfs:label  "Ensemble éditorial"@fr ,
                                          "Editorial set"@en ,
                                          "Insieme editoriale"@it ;

                              rdfs:comment  "L'ensemble éditorial auquel le document se rattache : 'Œuvre poétique', 'Journal', 'Périodique ou ouvrage collectif', 'Traductions', 'Divers'"@fr ,
                                            "The editorial set to which the document belongs: 'Œuvre poétique', 'Journal', 'Périodique ou ouvrage collectif', 'Traductions', 'Divers'"@en .


roud-oeuvres:isPhotocopy    rdf:type owl:ObjectProperty ;
                    
                            rdfs:subPropertyOf knora-base:hasValue ;

                            knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                            knora-base:objectClassConstraint knora-base:BooleanValue ;
                                            
