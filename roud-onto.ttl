roud-oeuvres:PageImage  rdf:type owl:Class ;
              
	                    rdfs:subClassOf knora-base:StillImageRepresentation ,
	                    [
	                      rdf:type owl:Restriction ;
	                      owl:onProperty roud-oeuvres:pageImageHasStillImageFileValue ;  ## sub-property of kb:hasStillImageFileValue, CHECK
	                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
	                    ];
	                                 
	                    rdfs:label    "Image de la page"@fr,
