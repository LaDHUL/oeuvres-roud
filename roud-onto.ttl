roud-oeuvres:hasDateWrittenSource   rdf:type owl:ObjectProperty ;

                                    rdfs:subPropertyOf knora-base:hasValue ;

                                    rdfs:subPropertyOf roud-oeuvres:hasDate ;

                                    knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                                    knora-base:objectClassConstraint knora-base:DateValue ;

                                    rdfs:label  "Date dans le manuscrit"@fr ,
                                                "Date on the manuscript"@en ,
                                                "Data sul manoscritto"@it ;

                                    rdfs:comment  "La date presente dans le manuscrit, s'il y en a"@fr ,
                                                  "The date present in the manuscript, if any"@en .


roud-oeuvres:hasDateEstablishedWrittenSource    rdf:type  owl:ObjectProperty ;

                                                rdfs:subPropertyOf knora-base:hasValue ;

                                                rdfs:subPropertyOf roud-oeuvres:hasDate ;

                                                knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                                                knora-base:objectClassConstraint knora-base:DateValue ;

                                                rdfs:label  "Date (hypothèse)"@fr ,
                                                            "Date (hypothesis)"@en ,
                                                            "Data (ipotesi)"@it ;

                                                rdfs:comment  "La date individué par les chercheures. À préciser aussi s'elle est égale à la date presente dans le manuscrit"@fr ,
                                                    "The date identified by the scholars. To be entered also when it is the same as the in the manuscrit"@en .


roud-oeuvres:hasGenre   rdf:type  owl:ObjectProperty ;
                      
                        rdfs:subPropertyOf  knora-base:hasValue ;

                        knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                        knora-base:objectClassConstraint knora-base:ListValue ;
                        ## See  roud-data-lists.ttl

                        rdfs:label  "Genre"@fr ,
                                    "Genre"@en ,
                                    "Genere"@it ;

                        rdfs:comment  "Le genre du texte : critique littéraire, critique d'art, chronique, oeuvre poétique, traduction, note, divers"@fr ,
                                      "The genre of the text: literary criticism, art criticism, chronicle, poetic work, translation, note, other"@en .



roud-oeuvres:hasGeneticStage  rdf:type  owl:ObjectProperty ;

                              rdfs:subPropertyOf knora-base:hasValue ;

                              knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                              knora-base:objectClassConstraint knora-base:ListValue ;
                              ## See  roud-data-lists.ttl

                              rdfs:label  "Étapes du processus d'écriture"@fr ,
                                          "Stages of the writing process"@en ,
                                          "Tappe nel processo di scrittura"@it ;

                              rdfs:comment  "Les étapes du processus d'écriture : note, plan, brouillon, mise au net, manuscrit définitif, corrections sur épreuves, original (tiré à part), original corrigé, autre"@fr ,
                                            "The genre of the text: note, plan, draft, clean manuscript, final manuscript, corrections on edited drafts, original (extract), original (extract) with corrections, other"@en .



roud-oeuvres:hasPublishedReference  rdf:type owl:ObjectProperty ;
                  
                                    rdfs:subPropertyOf knora-base:hasLinkTo ;       
                                    
                                    knora-base:subjectClassConstraint knora-base:WrittenSource ;
                                    
                                    ## roud-biblio ontology does not exist yet, but could be derived from other bibliographical ontologies in Knora.
                                    knora-base:objectClassConstraint roud-biblio:Document ;

                                    rdfs:label  "Référence bibliographique"@fr ,
                                                "Bibliographical reference"@en ;

                                    rdfs:comment  "La référence bibliographique (version publié ou autre)"@fr ,
                                                  "The bibliographical reference (published version or other)"@en .


roud-oeuvres:hasPublishedReferenceValue rdf:type owl:ObjectProperty ;
                      
                                        rdfs:subPropertyOf knora-base:hasLinkToValue ;        
                                        
                                        knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;
                                        
                                        knora-base:objectClassConstraint knora-base:LinkValue .



roud-oeuvres:isDigitized  rdf:type owl:ObjectProperty ;
                    
                          rdfs:subPropertyOf knora-base:hasValue ;

                          knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                          knora-base:objectClassConstraint knora-base:BooleanValue ;
                                          
                          rdfs:label  "Numérisé"@fr ,
                                      "Digitized"@en ,
                                      "Scannerizzato"@it ;
                          rdfs:comment "Le document a été numérisé"@fr .
                          ## This property has maxCardinality 1. If it's set to True is true, if it is not set is false. Is this the best way to structure something like an optional boolean value?
                          


roud-oeuvres:isToDigitize rdf:type owl:ObjectProperty ;
                    
                          rdfs:subPropertyOf knora-base:hasValue ;

                          knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                          knora-base:objectClassConstraint knora-base:BooleanValue ;
                                          
                          rdfs:label  "À numériser"@fr ,
                                      "To digitize"@en ,
                                      "Da scannerizzare"@it ;
                          
                          rdfs:comment "Le document a été numérisé"@fr .
                          ## This property has maxCardinality 1. If it's set to True is true, if it is not set is false. Is this the best way to structure something like an optional boolean value?
                          


roud-oeuvres:hasComment   rdf:type owl:ObjectProperty ;
                          
                          rdfs:subPropertyOf  knora-base:hasValue ;

                          knora-base:subjectClassConstraint roud-oeuvres:WrittenSource ;

                          knora-base:objectClassConstraint knora-base:TextValue ;

                          rdfs:label  "Commentaire"@fr ,
                                      "Comment"@en ,
                                      "Commento"@it ;

                          rdfs:comment  "Tout commentaire va dans ce champ, y compris les informations sur une datation incertaine, les relations avec autres textes, etc."@fr ,
                                        "All comments should go in this field, including information about uncertain dates, relations with other documents, etc."@en .


                          
roud-oeuvres:hasInternalComment   rdf:type owl:ObjectProperty ;
                          
                                  rdfs:subPropertyOf  knora-base:hasValue ;

