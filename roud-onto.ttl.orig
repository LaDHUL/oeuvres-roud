        "Œuvre d'art"@fr,
        "Opera d'arte"@it ;
    rdfs:comment "An artwork."@en,
        "Une œuvre d'art."@fr ;
    rdfs:subClassOf [ a owl:Restriction ;
            salsah-gui:guiOrder "4"^^xsd:nonNegativeInteger ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty roud-oeuvres:artworkHasPhoto ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty roud-oeuvres:artworkHasPhotoValue ],
        [ a owl:Restriction ;
            salsah-gui:guiOrder "5"^^xsd:nonNegativeInteger ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty roud-oeuvres:artworkHasLinkToDigitalFacsimile ],
        roud-oeuvres:Work,
        knora-base:Resource .

