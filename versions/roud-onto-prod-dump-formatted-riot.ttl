        rdfs:comment           "The date present in the manuscript, if any."@en , "La date presente dans le manuscrit, s'il y en a."@fr ;
        rdfs:label             "Date on the manuscript"@en , "Data sul manoscritto"@it , "Date dans le manuscrit"@fr ;
        rdfs:subPropertyOf     knora-base:hasValue ;
        knora-base:objectClassConstraint
                knora-base:TextValue ;
        knora-base:subjectClassConstraint
                roud-oeuvres:Manuscript ;
        salsah-gui:guiElement  salsah-gui:SimpleText .

roud-oeuvres:publicationHasInternalComment
        rdf:type               owl:ObjectProperty ;
        rdfs:comment           "Internal comment on the publication, for the project team only."@en , "Commentaire sur la publication qui est outil pour l'équipe travaillant sur le projet et qui ne sera pas publié."@fr ;
        rdfs:label             "Commentaire interne (reprises et remarques)"@fr , "Internal comment (reuse and remarks)"@en , "Commento interno (riusi e altre info)"@it ;
        rdfs:subPropertyOf     knora-base:hasValue ;
        knora-base:objectClassConstraint
                knora-base:TextValue ;
        knora-base:subjectClassConstraint
                roud-oeuvres:Publication ;
        salsah-gui:guiElement  salsah-gui:Richtext .

roud-oeuvres:Page  rdf:type  owl:Class ;
        rdfs:comment     "Une page."@fr , "A page."@en ;
        rdfs:label       "Page scannée d'un document ou d'une publication"@fr , "Digitized page of a document or publication"@en , "Pagina scannerizzata di un documento o di una pubblicazione"@it ;
        rdfs:subClassOf  knora-base:StillImageRepresentation ;
        rdfs:subClassOf  [ rdf:type             owl:Restriction ;
                           salsah-gui:guiOrder  "2"^^xsd:nonNegativeInteger ;
                           owl:cardinality      "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty       roud-oeuvres:pageHasName
                         ] ;
        rdfs:subClassOf  [ rdf:type             owl:Restriction ;
                           salsah-gui:guiOrder  "0"^^xsd:nonNegativeInteger ;
                           owl:maxCardinality   "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty       roud-oeuvres:pageIsPartOfManuscript
                         ] ;
        rdfs:subClassOf  [ rdf:type            owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      roud-oeuvres:pageIsPartOfPublicationValue
                         ] ;
        rdfs:subClassOf  [ rdf:type             owl:Restriction ;
                           salsah-gui:guiOrder  "1"^^xsd:nonNegativeInteger ;
                           owl:maxCardinality   "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty       roud-oeuvres:pageIsPartOfPublication
                         ] ;
        rdfs:subClassOf  [ rdf:type             owl:Restriction ;
                           salsah-gui:guiOrder  "3"^^xsd:nonNegativeInteger ;
                           owl:cardinality      "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty       roud-oeuvres:hasSeqnum
                         ] ;
        rdfs:subClassOf  [ rdf:type            owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      roud-oeuvres:pageIsPartOfManuscriptValue
                         ] .

