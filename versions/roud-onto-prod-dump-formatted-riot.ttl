                roud-oeuvres:MsPart ;
        knora-base:subjectClassConstraint
                roud-oeuvres:MsPart ;
        salsah-gui:guiElement  salsah-gui:Searchbox .

roud-oeuvres:hasDhsID
        rdf:type               owl:ObjectProperty ;
        rdfs:comment           "Lien à la notice de la personne dans le dictionnaire historique de la Suisse."@fr , "Link to the information about this person in Dictionnaire historique de la Suisse (URL)."@en ;
        rdfs:label             "(Propriété abstraite, seulement pour les requêtes) Dictionnaire historique de la Suisse (URL)"@fr , "(Proprietà astratta, solo per ricerca) Dizionario storico della Svizzera (URL)"@it , "(Abstract property, only for querying) Dictionnaire historique de la Suisse (URL)"@en ;
        rdfs:subPropertyOf     knora-base:hasValue ;
        knora-base:objectClassConstraint
                knora-base:UriValue ;
        salsah-gui:guiElement  salsah-gui:SimpleText .

roud-oeuvres:msPartIsReusedInDossierPartValue
        rdf:type            owl:ObjectProperty ;
        rdfs:label          "é usata in una parte di un dossier genetico"@it , "is used in a part of a genetic dossier"@en , "est utilisée dans une partie d'un dossier génétique"@fr ;
